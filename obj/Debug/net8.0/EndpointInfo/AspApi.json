      "CreateStockRequestDto": {
        "required": [
          "purchase"
        ],
        "type": "object",
        "properties": {
          "symbol": {
            "type": "string",
            "nullable": true
          },
          "companyName": {
            "type": "string",
            "nullable": true
          },
          "purchase": {
            "maximum": 1000,
            "minimum": 0.01,
            "type": "number",
            "format": "double"
          },
          "lastDiv": {
            "type": "number",
            "format": "double"
          },
          "industry": {
            "type": "string",
            "nullable": true
          },
          "marketCap": {
            "type": "integer",
            "format": "int64"
          }
        },
        "additionalProperties": false
      },
      "RegisterDto": {
        "required": [
          "email",
          "password",
          "userName"
        ],
        "type": "object",
        "properties": {
          "userName": {
            "maxLength": 50,
            "minLength": 1,
            "type": "string"
          },
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "password": {
            "minLength": 6,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "UpdateCommentDto": {
        "required": [
          "title"
        ],
        "type": "object",
        "properties": {
          "title": {
            "maxLength": 100,
            "minLength": 1,
